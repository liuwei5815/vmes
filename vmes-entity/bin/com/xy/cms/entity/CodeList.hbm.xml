<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.xy.cms.entity.CodeList" table="sys_code_list">
		<composite-id>
			<key-property name="code" type="java.lang.String">
				<column name="code" length="20" />
			</key-property>
			<key-property name="typeId" type="java.lang.String">
				<column name="type_id" length="20" />
			</key-property>
		</composite-id>
		<property name="contents" type="java.lang.String">
			<column name="contents" length="30" />
		</property>

	</class>
</hibernate-mapping>
