<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.xy.cms.entity.TableColumnType" table="sys_table_column_type" dynamic-insert="true" dynamic-update="true">
		<composite-id>
			<key-property name="code" type="java.lang.Long">
				<column name="code" />

			</key-property>
			<key-property name="name" type="java.lang.String">
				<column name="name">
					<comment>字段类型名</comment>
				</column>

			</key-property>
		</composite-id>

		<property name="type" type="java.lang.String">
					<column name="type">
						<comment>说明</comment>
					</column>
		</property>
		</class>
</hibernate-mapping>
