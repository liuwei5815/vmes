<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.xy.cms.entity.Customer" table="customer">
		<id name="id" type="java.lang.Long">
            <column name="id">
             </column>
            <generator class="native" />
        </id>
        <property name="name" type="java.lang.String">
            <column name="name" />
        </property>
        <property name="tel" type="java.lang.String">
            <column name="tel" />
        </property>
        <property name="contact" type="java.lang.String">
            <column name="contact" />
        </property>
        <property name="addDate" type="java.sql.Timestamp">
            <column name="add_date">
                <comment>创建时间</comment>
            </column>
        </property>
		<property name="updateDate" type="java.sql.Timestamp">
            <column name="update_date">
                <comment>修改时间</comment>
            </column>
        </property>
        <property name="state" type="java.lang.Integer">
            <column name="state">
                <comment>状态</comment>
            </column>
        </property>
         <property name="customerCode" type="java.lang.String">
             <column name="customerCode">
                <comment>状态</comment>
            </column>
        </property>
        <property name="typeId" type="java.lang.String">
             <column name="type_id">
                <comment>客户类型</comment>
            </column>
        </property>
        <property name="departmentId" type="java.lang.Long">
             <column name="department_id">
                <comment>组织id</comment>
            </column>
        </property>  
        <property name="activeState" type="java.lang.Integer">
            <column name="active_state">
                <comment>激活状态</comment>
            </column>
        </property>  
 
	</class>
</hibernate-mapping>
