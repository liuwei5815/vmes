<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.xy.cms.entity.EquipmentParam" table="equipment_parameter" dynamic-insert="true" dynamic-update="true">
		<id name="paremtId" type="java.lang.Long">
            <column name="id" />
        </id>
        <property name="paramterName" type="java.lang.String">
            <column name="name">
                <comment>参数名称</comment>
            </column>
        </property>
	</class>
</hibernate-mapping>
