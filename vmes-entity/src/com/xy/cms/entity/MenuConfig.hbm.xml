<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.xy.cms.entity.MenuConfig" table="sys_menu_config" dynamic-insert="true">
		<id name="id" type="java.lang.Long">
            <column name="id" />
            <generator class="native" />
        </id>
        <property name="layerWidth" type="java.lang.Long">
            <column name="layer_width">
                <comment>弹框宽度</comment>
            </column>
        </property>
        <property name="layerHeight" type="java.lang.Long">
            <column name="layer_height">
                <comment>弹框高度</comment>
            </column>
        </property>
        <property name="menuId" type="java.lang.Long">
            <column name="menu_id">
                <comment>菜单ID</comment>
            </column>
        </property>
	</class>
</hibernate-mapping>
