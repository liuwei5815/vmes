<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.xy.cms.entity.MaterialType" table="material_type" dynamic-insert="true" dynamic-update="true">
		<id name="id" type="java.lang.Long">
            <column name="id" />
            <generator class="native" />
        </id>
        <property name="name" type="java.lang.String">
            <column name="name" length="30">
                <comment>物料类型名称</comment>
            </column>
        </property>
          <property name="model" type="java.lang.String">
            <column name="model" length="10">
                <comment>物料类型</comment>
            </column>
        </property> 
        <property name="img" type="java.lang.String">
            <column name="img" length="50">
                <comment>物料类型图片</comment>
            </column>
        </property>
         <property name="pid" type="java.lang.Integer">
            <column name="pid" length="12">
                <comment>物料类型型号</comment>
            </column>
        </property>
	</class>
</hibernate-mapping>
